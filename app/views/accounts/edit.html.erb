<%= form_with model: @account, local: true do |f| %>
      <% if @account.errors.any? %>
        <div id="explain_error">
          <h2>
            Uh oh!
            <%= pluralize(@account.errors.count, "error") %>
            prohibited this post from being saved:
          </h2>
          <ul>
            <% @account.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
      <% end %>
      <div>
        <div class="field">
          <%= f.label "Account Name:" %> <br>
          <%= f.text_field :name %>
      </div>
        <br>
        <div class="field">
          <%= f.label "Account Type:" %> <br>
          <%= f.select :account_type, acct_type_list%>
        </div>
        <br>
      </div>

        <%= f.submit "edit account", class: 'btn btn-primary'%>
      <% end %>
